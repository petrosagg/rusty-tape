{
  "name": "rusty-tape",
  "version": "0.1.0",
  "scripts": {
    "start": "webpack serve",
    "build": "webpack",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "ignorePatterns": [
      "dist",
      "node_modules"
    ],
    "plugins": [
      "prettier"
    ],
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "prettier/prettier": "error",
      "max-len": [
        "error",
        {
          "code": 100
        }
      ]
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ]
  },
  "dependencies": {
    "buffer": "^5.7.1",
    "moment": "^2.29.1",
    "process": "^0.11.10",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^2.0.3",
    "stream-browserify": "^3.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "css-loader": "^5.2.0",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.23.1",
    "html-webpack-plugin": "^5.3.1",
    "prettier": "^2.2.1",
    "style-loader": "^2.0.0",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.31.0",
    "webpack-cli": "^4.6.0",
    "webpack-dev-server": "^3.11.2"
  }
}
